            <a href="../views/signup.ejs" id="sign">Sign-up/Login</a>
        </div>
        
        <h1>AVAILABLE TRAINS</h1>
        <table>
            <thead>
                <tr>
                    <th>CLASS</th>
                    <th>TRAIN NUMBER</th>
                    <th>TRAIN NAME</th>
                    <th>DEPARTURE FROM</th>
                    <th>ARRIVAL TO</th>
                    <th>DATE OF DEPARTURE</th>
                    <th>DATE OF RETURN</th>
                    <th>PRICE</th>
                </tr>
            </thead>
            <tbody>
                <% data.forEach(function(value) { %>
                    <tr>
                        <td><%= value.Class %></td>
                        <td><%= value.TrainNumber %></td>
                        <td><%= value.TrainName %></td>
                        <td><%= value.From %></td>
                        <td><%= value.To %></td>
                        <td><%= value.DepartDate %></td>
                        <td><%= value.ReturnDate %></td>
                        <td><%= value.Price %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>


